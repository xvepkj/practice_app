<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practice_app">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practice_app">
        <activity
            android:name=".FontActivity"
            android:exported="true" />
        <activity
            android:name=".AsyncTaskActivity"
            android:exported="true" />
        <activity
            android:name=".CoilActivity"
            android:exported="true" />
        <activity
            android:name=".FrescoActivity"
            android:exported="true" />
        <activity
            android:name=".PicassoActivity"
            android:exported="true" />
        <activity
            android:name=".GlideActivity"
            android:exported="true" />
        <activity
            android:name=".BindingActivity"
            android:exported="true" />

        <service
            android:name=".LocalService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".ForegroundService"
            android:exported="true" />

        <activity
            android:name=".FSActivity"
            android:exported="true" />
        <activity
            android:name=".VBActivity"
            android:exported="true" />
        <activity android:name=".NavigationContainer" />
        <activity android:name=".LifeCycle" />
        <activity android:name=".ServiceActivity" />
        <activity android:name=".IntentActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".NewService" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>